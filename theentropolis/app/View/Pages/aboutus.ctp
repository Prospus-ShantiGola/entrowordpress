<script>
   $('#a-reload').click(function() {
   var $captcha = $("#img-captcha");
   $captcha.attr('src', $captcha.attr('src') + '?' + Math.random());
   return false;
   });
   
   $('#aboutUsForm').submit(function(event) {
   event.preventDefault();
   var datas = $('#aboutUsForm').serialize();
   $.ajax({
           url: "<?php echo Router::url(array('controller' => 'Pages', 'action' => 'aboutus')) ?>",
           data: datas,
           type: 'POST',
           success: function (data) {
               clearAllError();
               if (data.result == "success") {
                   // bootbox.alert("GREAT TO HEAR FROM YOU<br>Your message has been sent to TrepiCity | HQ. We will be in touch in the next 24 hours. <br><br>The Team at Trepicity | HQ");
                   $('#about-thanks-msg').modal('show');
                   $("#aboutUsForm").get(0).reset();
               } else {
                   if (data.error_msg.name !== undefined && data.error_msg.name[0] != '') {
                       $("#name").after('<div class="error-message">' + data.error_msg.name[0] + '</div>');
                   }
                   if (data.error_msg.last_name !== undefined && data.error_msg.last_name[0] != '') {
                       $("#last_name").after('<div class="error-message">' + data.error_msg.last_name[0] + '</div>');
                   }
                   if (data.error_msg.email_address !== undefined && data.error_msg.email_address[0] != '') {
                       $("#email_address").after('<div class="error-message">' + data.error_msg.email_address[0] + '</div>');
                   }
                   if (data.error_msg.subject !== undefined && data.error_msg.subject[0] != '') {
                       $("#subject").after('<div class="error-message">' + data.error_msg.subject[0] + '</div>');
                   }
                   if (data.error_msg.message !== undefined && data.error_msg.message[0] != '') {
                       $("#message").after('<div class="error-message">' + data.error_msg.message[0] + '</div>');
                   }
                   if (data.error_msg.captcha !== undefined && data.error_msg.captcha[0] != '') {
                       $("#captcha").after('<div class="error-message">' + data.error_msg.captcha[0] + '</div>');
                   }
               }
           }
   });
   return false;
   });
   
   function clearAllError() {
       $("#name").nextAll().remove();
       $("#last_name").nextAll().remove();
       $("#email_address").nextAll().remove();
       $("#subject").nextAll().remove();
       $("#message").nextAll().remove();
       $("#captcha").nextAll().remove();
   }
   function refreshcaptcha() {
      var image = document.getElementById("img-captcha");
      image.setAttribute("src", "<?php echo Router::url(array('controller' => 'Pages', 'action' => 'captcha')) ?>?"+(new Date()).getTime());
   }
</script>