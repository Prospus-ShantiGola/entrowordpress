
* {
  box-sizing: border-box;
}
html{
    font-family: 'Source Sans Pro', sans-serif;
    height: 100%;
    font-size:  1rem;
    font-weight:400;
    color: #414753;
    margin: 0rem;
    padding:0rem; 
    background: #f1f2f7;
}
body{
    font-family: 'Source Sans Pro', sans-serif;
    font-weight: 600;
    font-size: 14px;
    color: #7e9097;
}

/*flex container using in main parent div */
.flex-container {
    display: flex;
}
/*end*/

/*Main Side Wrapper*/
.sidebar-wrapper {
    width: 17.50rem;
}
.sidebar-bar {
    height: 100%;
}
/*==========left menu flyout start==================*/
.sidebar-wrapper .close-toggle-fly {
    display: none;
    background: #414753;
    padding: 5px 15px;
}
.sidebar-wrapper.fixedSidebarLeftFlyout.open .user-profile-wrap{
    padding-top:2rem;
}
.sidebar-wrapper .close-toggle-fly span {
    color: #fff;
    font-size: 20px;
    text-transform: uppercase;
    cursor: pointer;
}
.close-md-white {
    background-position: -21px -197px;
    height: 22px;
    position: relative; 
    top: 2px; 
    margin-right: 10px; 
    display: inline-block;
}
.menu-bg-overlay {
    position: fixed;
    z-index: 9;
    background: rgba(0,0,0,0.5);
    width: 100%;
    height: 100%;
    display: none;
    top: 0px;
}
/*========left menu flyout end====================*/
/* User profile wrap*/
.user-profile-wrap {
    padding:3.75rem 1.56rem 2.81rem 1.56rem ;
}
.user-profile-pic {
    display: flex;
    justify-content: center;
}
.user-profile-pic img {
    border-radius: 4px;
}

.user-avatar-name {
    text-align: center;
    margin: 0.71rem 0 1.88rem 0;
    text-transform: uppercase;
    color: #fff;
    font-weight: 600;
    font-size: 1.14rem
}
    /* End*/

/*Navigation wrap*/

.navigation-sidebar .nav  li  a {
    display: flex;
    padding: 0.71rem 1.56rem;
    color: #fff;
    font-size: 1rem;
    border-left-width: 0.43rem;
    border-color: transparent;
    border-style: solid;
    border-bottom: 0;
    border-top-width: 0;
    align-items: center;
    border-right-width: 0;
    text-decoration: none;
     -webkit-transition: all .25s linear;
    -moz-transition: all .25s linear;
    -ms-transition: all .25s linear;
    -o-transition: all .25s linear;
    transition: all .25s linear;
}
.navigation-sidebar .nav li a:hover, 
.navigation-sidebar .nav li a:focus, 
.navigation-sidebar .nav li a:active {
    background: #555b67;
    border-color: red;
}

    /*End*/

/*siderbar footer*/
/*.footer-sidebar {
    text-align: center;
    padding: 0.71rem 1.79rem;
}*/
.footer-sidebar {
    text-align: center;
    padding: 12.71rem 1.79rem 0.71rem 1.79rem;
}
.footer-sidebar hr {
    margin: 0.62rem 0;147
}
.footer-social-links a {
    color: #fff;
    margin: 0 10px;
}

    /*end*/

/*   main sidebar End       */

/* main wraper*/
#kid-menu{
    height: 100vh;
}
.toggle-menu-fly{
    display: none;
    float: left;
}
.main-wrapper {
  /*  padding: 1.56rem;*/
    width: calc(100% - 17.50rem);
    background: #f1f2f7;
}


/*page-heading-strip */
.kd-page-heading-strip {
    padding: 0.62rem 0px;
    color: #000;
    border-bottom: 1px solid #000;
    font-size: 1.38rem;
    font-weight: 600;
    display: flex;
    text-transform: uppercase;
    align-items: center;
    position: relative;
}
/*page-heading-strip  end*/


/*Middle Area*/
.middle-flex-container {
    display: flex;
    margin: 1.25rem 0;
}
.middle-flex-item-first {
    width: calc(100% - 25.00rem);
}
.middle-flex-item {
    width:  25.00rem;
    display: block;
}
.multiple-section-flex{
    display: flex;
}
.flex-item-section {
    flex: 1;
}
.flex-item-section:first-child {
    margin-right: 1.25rem;
}
.flex-item-section:last-child {
    margin-right: 1.25rem;
}


/*Profile section*/

/* .kd-layout-header{
    height: 37px;
    background: #414753;
 }*/
.profile-avatar-col {
    display: flex;    
    align-items: flex-start;
    justify-content: center;
    width: 9.38rem;
    border: 1px #eee solid;
    margin-right: 20px;
}
.profile-avatar-description-col > ul {
    margin: 0;
    padding: 0;
}
.profile-avatar-description-col ul > li:first-child {
    text-transform: uppercase;
    font-weight: bold;
    font-size: 1.12rem
}

.profile-avatar-description-col ul > li {
    margin-bottom: 0.62rem;
}

.profile-avatar-description-col {
    flex: 1;
}
.action-strip {
    flex: 1 100%;
    order: 4;
    display: flex;
    margin: 0.62rem;
    justify-content: space-between;
}
.action-strip > div {
    width: 48%;
}
/* profile End */

/*suggestion section*/
.suggestion-section {
    margin: 1.25rem 0 0;
}
.suggestion-aside form {
    width: 100%;
}
.suggestion-aside textarea {
    min-height: 5.00rem;
    margin-bottom: 0.94rem
}
/*End*/

.business-section .flex-item {
    flex: 1;
}  
.business-section .flex-item:first-child {
    margin-right: 1.25rem;
}

/*Ask | HQ form + counter-section*/
.aks-section, .counter-section {
    margin-top: 1.25rem;
    margin-right: 1.25rem;
}
.ask-form {
    width: 100%;
}
.ask-form > div {
    margin-bottom: 0.62rem;
}
.ask-form textarea {
    min-height: 5.00rem;
    margin-bottom: 0.5em;
}
.flex-counter {
    flex: 1 100%;
    display: flex;
    justify-content: space-between;
}
.flex-col {
    border: 1px solid #eee;
    height: 120px;
    text-align: center;
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 32.8%;
    justify-content: center;
}
.flex-col > span {
    font-size: 3.12rem;
    font-weight: bold;
    color: #0bb6ea;
    line-height: 110%;
}
.flex-col > span.camp-name {
    font-size: 1rem;
    text-transform: uppercase;
}

/*End*/


/*Tabbing on right side*/
.tabbing-section .nav-tabs {
    display: flex;
}

.tabbing-section .nav-tabs li {
    float: none;
    margin: 0;
    flex: 1;
}
.tabbing-section .nav > li > a {
    position: relative;
    display: block;
    padding: 10px 15px;
    background: #bec7cb;
    border-radius: 0;
    border: 0;
    margin: 0;
    color: #fff;
    text-transform: uppercase;
    font-weight: 600;
}

.tabbing-section .nav-tabs > li.active > a, 
.tabbing-section .nav-tabs > li.active > a:hover, 
.tabbing-section .nav-tabs > li.active > a:focus {
    color: #fff;
    cursor: auto;
    background-color: #414753;
     border: 0;
  
    border-bottom-color: transparent;
   
}
.tabbing-section .nav > li > a:hover, .tabbing-section .nav > li > a:focus {
    text-decoration: none;
    background-color: #bec7cb;
}
.tab-content {
    background: #fff;
    
}
